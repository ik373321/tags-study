<%= form_with model:@tweet,  local: true do |f| %>
      <%= render 'shared/error_messages', model: f.object %>
<div class="mb-3">
            <label for="exampleFormControlTextarea1" class="form-label">メモ　必須</label>
            <%= f.text_area :memo,class:"form-control", id:"exampleFormControlTextarea1", rows:"3", placeholder:"例）メモ"%>
          </div>
      <div class="sell-btn-contents">
         <%= f.submit "投稿する" ,class:"btn  btn-outline-secondary" %>
         <%=link_to 'もどる', root_path, class:"btn btn-outline-primary" %>
      </div>
 <% end %>